@model IEnumerable<finalProject.Models.ApplicationUser>
@{
    ViewBag.Title = "UserList";
}
<style>
    th, td {
        text-align: center;
    }
</style>
<h2>UserList</h2>
    <table>
            <tr>
                <td>@Html.DisplayName("UserName")</td>
                <td></td>
                <td></td>


                <td>@Html.DisplayName("Email")</td>

            </tr>
            @foreach (var item in Model)

            {
                <tr>
                    <td>@Html.DisplayFor(model => item.UserName)</td>
                    <td></td>
                    <td></td>

                    <td>@Html.DisplayFor(model => item.Email)</td>
                    <td></td>
                    <td></td>
                    <td>@Html.ActionLink("Delete", "UserDelete", new { id = item.Id })</td>
                    <td>@Html.ActionLink("Edit", "UserEdit", new { id = item.Id })</td>
                </tr>
            }

    </table>
